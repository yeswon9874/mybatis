<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.tain.mybatis.mappers.UsrMapper">
	
	<select id="selectAllUsr" resultType="Usr">
		select
			id
			, user_id
			, pass_wd
			, desc
		from
			tb_usr
		where 1=1
	</select>
	
</mapper>
